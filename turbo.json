{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**"]
        },
        "test": {},
        "dev": {
            "cache": false
        },
        "format": {
            "outputs": ["**/*.{js,jsx,ts,tsx,json,css,md}"],
            "command": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,md}'"
        }
    }
}
